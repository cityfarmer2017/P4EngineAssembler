// send meta original without change
SNDM ;

// set start_offset_r in meta to 0
SETH POFF, 0;
SETL POFF, 0;

// send header from original packet header
COPY META{48:2}, META[170]; // set length_r with META{header_len}
MSKALL ;
SNDH HDR;

// send payload from original pakcet payload, with CRC32 result
COPY META{52:4}, META[170]; // set length_r with META{payload_len}
MSKALL ;
SNDPC PLD, CRC32;

END ;